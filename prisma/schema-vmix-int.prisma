generator client {
  provider = "prisma-client-js"
  output   = "./generated/vmix_int"
  binaryTargets = ["native", "rhel-openssl-1.0.x", "windows"]
}

datasource db {
  provider = "sqlserver"
  url      = env("DEV_DB_VISUAL_MIX_INT_URL")
}

model CLIENTE_ESTADO_SAP {
  CODIGO           BigInt    @id(map: "PK_CLIENTE_ESTADO_SAP") @default(0, map: "DF_CLIENTE_ESTADO_SAP_CODIGO")
  ESTADO           String?   @default(" ", map: "DF_CLIENTE_ESTADO_SAP_ESTADO") @db.VarChar(2)
  CODIGOINTEGRACAO String?   @default(" ", map: "DF_CLIENTE_ESTADO_SAP_CODIGOINTEGRACAO") @db.VarChar(20)
  DATAALTERACAO    DateTime? @default(dbgenerated("1980/01/01 00:00:00"), map: "DF_CLIENTE_ESTADO_SAP_DATAALTERACAO") @db.DateTime
}

model CONTROLE_IMPPRODUTOS {
  DATA_IMPORTACAO DateTime  @id(map: "PK_CONTROLE_IMPPRODUTOS") @default(dbgenerated("1980/01/01 00:00:00"), map: "DF_CONTROLE_IMPPRODUTOS_DATA_IMPORTACAO") @db.DateTime
  DATA_CUSTO      DateTime? @default(dbgenerated("1980/01/01 00:00:00"), map: "DF_CONTROLE_IMPPRODUTOS_DATA_CUSTO") @db.DateTime
}

model DEVOLUCAO {
  LOJA           Int?      @default(0, map: "DF_DEVOLUCAO_LOJA")
  CODIGO         BigInt    @id(map: "PK_DEVOLUCAO") @default(0, map: "DF_DEVOLUCAO_CODIGO")
  VALORDEVOLUCAO Float?    @default(0, map: "DF_DEVOLUCAO_VALORDEVOLUCAO") @db.Money
  SITUACAO       Int?      @default(0, map: "DF_DEVOLUCAO_SITUACAO") @db.TinyInt
  SITUACAO_DATA  DateTime? @default(dbgenerated("1980/01/01 00:00:00"), map: "DF_DEVOLUCAO_SITUACAO_DATA") @db.DateTime
  SITUACAO_LOJA  Int?      @default(0, map: "DF_DEVOLUCAO_SITUACAO_LOJA") @db.SmallInt
  SITUACAO_PDV   Int?      @default(0, map: "DF_DEVOLUCAO_SITUACAO_PDV") @db.SmallInt
  SITUACAO_COO   Int?      @default(0, map: "DF_DEVOLUCAO_SITUACAO_COO")
  DATAINCLUSAO   DateTime? @default(dbgenerated("1980/01/01 00:00:00"), map: "DF_DEVOLUCAO_DATAINCLUSAO") @db.DateTime
}

model EXPORTACAO {
  LOJA           Int       @default(0, map: "DF_EXPORTACAO_LOJA") @db.SmallInt
  TIPO_CADASTRO  Int       @default(0, map: "DF_EXPORTACAO_TIPO_CADASTRO") @db.TinyInt
  CHAVE          String    @default("", map: "DF_EXPORTACAO_CHAVE") @db.VarChar(40)
  DATA_INCLUSAO  DateTime? @default(now(), map: "DF_EXPORTACAO_DATA_INCLUSAO") @db.DateTime
  DATA_INIPROC   DateTime? @default(now(), map: "DF_EXPORTACAO_DATA_INIPROC") @db.DateTime
  DATA_FIMPROC   DateTime? @default(now(), map: "DF_EXPORTACAO_DATA_FIMPROC") @db.DateTime
  INTEGRADO      Int       @default(0, map: "DF_EXPORTACAO_INTEGRADO") @db.SmallInt
  DESC_INTEGRADO String?   @default(" ", map: "DF_EXPORTACAO_DESC_INTEGRADO") @db.VarChar(255)
  DATA1          DateTime? @default(dbgenerated("1980/01/01 00:00:00"), map: "DF_EXPORTACAO_DATA1") @db.DateTime
  DATA2          DateTime? @default(dbgenerated("1980/01/01 00:00:00"), map: "DF_EXPORTACAO_DATA2") @db.DateTime
  DATA3          DateTime? @default(dbgenerated("1980/01/01 00:00:00"), map: "DF_EXPORTACAO_DATA3") @db.DateTime
  VALOR1         Float?    @default(0, map: "DF_EXPORTACAO_VALOR1")
  VALOR2         Float?    @default(0, map: "DF_EXPORTACAO_VALOR2")
  VALOR3         Float?    @default(0, map: "DF_EXPORTACAO_VALOR3")
  VALOR4         Float?    @default(0, map: "DF_EXPORTACAO_VALOR4")
  VALOR5         Float?    @default(0, map: "DF_EXPORTACAO_VALOR5")
  VALOR6         Float?    @default(0, map: "DF_EXPORTACAO_VALOR6")
  PROCESSO       String?   @default(" ", map: "DF_EXPORTACAO_PROCESSO") @db.VarChar(20)
  VALOR7         Float?    @default(0, map: "DF_EXPORTACAO_VALOR7")

  @@id([LOJA, TIPO_CADASTRO, CHAVE], map: "PK_EXPORTACAO")
  @@index([TIPO_CADASTRO, DATA_INCLUSAO, INTEGRADO], map: "_dta_index_EXPORTACAO_11_2105058535__K2_K4_K7_1_3_5_6_8_9_10_11_12_13_14_15_16_17_18_19")
  @@index([INTEGRADO, LOJA, TIPO_CADASTRO, CHAVE, DATA_INCLUSAO, VALOR1, VALOR2, VALOR3, VALOR4, VALOR5, VALOR6], map: "_dta_index_EXPORTACAO_11_2105058535__K7_K1_K2_K3_K4_K12_K13_K14_K15_K16_K17_5_6_8_9_10_11_18_19")
  @@index([INTEGRADO], map: "IX_EXPORTACAO")
}

model FINALIZADORAS_SAP {
  COD_FINALIZ   Int     @default(0, map: "DF_FINALIZADORAS_SAP_COD_FINALIZ") @db.TinyInt
  DOMINIO       Int     @default(0, map: "DF_FINALIZADORAS_SAP_DOMINIO") @db.SmallInt
  TRANSACAO     Int     @default(0, map: "DF_FINALIZADORAS_SAP_TRANSACAO") @db.SmallInt
  TIPO          Int?    @default(0, map: "DF_FINALIZADORAS_SAP_TIPO") @db.TinyInt
  DESCRICAO     String? @default(" ", map: "DF_FINALIZADORAS_SAP_DESCRICAO") @db.VarChar(30)
  OPERACAO_SAP  String? @default(" ", map: "DF_FINALIZADORAS_SAP_OPERACAO_SAP") @db.VarChar(2)
  OPERADORA_SAP String? @default(" ", map: "DF_FINALIZADORAS_SAP_OPERADORA_SAP") @db.VarChar(10)
  REDE          Int     @default(0, map: "DF_FINALIZADORAS_SAP_REDE") @db.SmallInt
  BANDEIRA      Int     @default(0, map: "DF_FINALIZADORAS_SAP_BANDEIRA") @db.SmallInt
  MODALIDADE    Int     @default(0, map: "DF_FINALIZADORAS_SAP_MODALIDADE") @db.SmallInt
  BIN           Int     @default(0, map: "DF_FINALIZADORAS_SAP_BIN") @db.SmallInt

  @@id([COD_FINALIZ, DOMINIO, TRANSACAO, REDE, BANDEIRA, MODALIDADE, BIN], map: "PK_FINALIZADORAS_SAP")
}

model MONITORAMENTO {
  THREAD    String    @id(map: "PK_MONITORAMENTO") @default("", map: "DF_MONITORAMENTO_THREAD") @db.VarChar(20)
  DATA_LOG  DateTime? @default(now(), map: "DF_MONITORAMENTO_DATA_LOG") @db.DateTime
  DESCRICAO String?   @default(" ", map: "DF_MONITORAMENTO_DESCRICAO") @db.VarChar(255)
  STATUS    Int?      @default(0, map: "DF_MONITORAMENTO_STATUS") @db.TinyInt
}

model PARAMETROS_SAP {
  ID_PARAMETRO Int     @id(map: "PK_PARAMETROS_SAP") @default(0, map: "DF_PARAMETROS_SAP_ID_PARAMETRO")
  DESCRICAO    String? @default(" ", map: "DF_PARAMETROS_SAP_DESCRICAO") @db.VarChar(50)
  PARAMETRO    String? @default(" ", map: "DF_PARAMETROS_SAP_PARAMETRO") @db.VarChar(255)
  TIPO         Int?    @default(0, map: "DF_PARAMETROS_SAP_TIPO") @db.TinyInt
}

model PEDIDO_CONTROLE {
  NUM_PEDIDO BigInt @id(map: "PK_PEDIDO_CONTROLE") @default(0, map: "DF_PEDIDO_CONTROLE_NUM_PEDIDO")
  LOJA       Int?   @default(0, map: "DF_PEDIDO_CONTROLE_LOJA")
  NUM_PDV    Int?   @default(0, map: "DF_PEDIDO_CONTROLE_NUM_PDV") @db.TinyInt
  SITUACAO   Int?   @default(0, map: "DF_PEDIDO_CONTROLE_SITUACAO") @db.TinyInt
}
